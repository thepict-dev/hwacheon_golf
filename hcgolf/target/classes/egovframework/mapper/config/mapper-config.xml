<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>
	
	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="egovMap" 			type="egovframework.rte.psl.dataaccess.util.EgovMap" />
		<typeAlias alias="FileVO"			type="egovframework.com.cmm.service.FileVO" />
		<typeAlias alias="ComDefaultCodeVO" type="egovframework.com.cmm.ComDefaultCodeVO" />
		<typeAlias alias="comDefaultVO"		type="egovframework.com.cmm.ComDefaultVO" />
				
		<typeAlias alias="BoardVO"			type="egovframework.com.cop.bbs.service.BoardVO" />
		<typeAlias alias="BoardItemVO"		type="egovframework.com.cop.bbs.service.BoardItemVO" />
		<typeAlias alias="BoardCateVO"		type="egovframework.com.cop.bbs.service.BoardCateVO" />
		
		<typeAlias alias="AdminVO"			type="egovframework.breeze.admin.service.AdminVO" />
		<typeAlias alias="MemberVO"			type="egovframework.breeze.member.service.MemberVO" />
		<typeAlias alias="LayoutVO"			type="egovframework.breeze.site.service.LayoutVO" />
		<typeAlias alias="AnalyticsVO"		type="egovframework.breeze.site.service.AnalyticsVO" />
		<typeAlias alias="MenuVO"			type="egovframework.breeze.site.service.MenuVO" />
		<typeAlias alias="ContentsVO"		type="egovframework.breeze.site.service.ContentsVO" />
		<typeAlias alias="AtchVO"			type="egovframework.breeze.site.service.AtchVO" />
		<typeAlias alias="PopupVO"			type="egovframework.breeze.popup.service.PopupVO" />
		<typeAlias alias="AccessVO"			type="egovframework.breeze.secure.service.AccessVO" />
		<typeAlias alias="DefaultVO"		type="egovframework.breeze.secure.service.DefaultVO" />
		<typeAlias alias="SurveyVO"			type="egovframework.breeze.survey.service.SurveyVO" />
		<typeAlias alias="SkinVO"			type="egovframework.breeze.skin.service.SkinVO" />
		<typeAlias alias="CodeVO"			type="egovframework.breeze.code.service.CodeVO" />
		<typeAlias alias="IndexVO"			type="egovframework.breeze.core.service.IndexVO" />
		
		<typeAlias alias="EventVO"			type="egovframework.golf.service.EventVO" />
		
		
	</typeAliases>
    
</configuration>